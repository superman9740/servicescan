<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

 <navigation-rule>
        <display-name>login</display-name>
       <from-view-id>/faces/testjsf.xhtml</from-view-id>
       <navigation-case>
          <from-action>#{userLoginBean.validateLogin}</from-action>
          <from-outcome>main</from-outcome>
          <to-view-id>/faces/main.xhtml</to-view-id>
          <redirect/>
       </navigation-case>
       
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>manageContractorBean</managed-bean-name>
        <managed-bean-class>com.dickson.servicescanserverside.beans.ManageContractorBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
